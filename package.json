  "bugs": {
    "url": "https://github.com/webdemos/cucumber-demo/issues"
  },
  "homepage": "https://github.com/webdemos/cucumber-demo#readme",
  "devDependencies": {
    "ace-builds": "1.2.5",
    "ansi_up": "1.3.0",
    "bootstrap": "3.3.7",
    "browser-sync": "2.17.3",
    "browser-sync-webpack-plugin": "1.1.3",
    "bundle-loader": "0.5.4",
    "css-loader": "0.25.0",
    "cucumber": "1.3.1",
    "file-loader": "0.9.0",
    "fs-extra": "0.30.0",
    "jquery": "3.1.1",
    "raw-loader": "0.5.1",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
