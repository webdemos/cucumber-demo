{
  "name": "cucumber-demo",
  "version": "0.0.1",
  "description": "A demo to show how to use cucumberjs in browser side",
  "main": "index.js",
  "scripts": {
    "ut:cucumber:create-steps": "node ./step_factories/.bin/generate-steps.js",
    "ut:cucumber:create-steps:debug": "node-debug ./step_factories/.bin/generate-steps.js",
    "webpack": "webpack --watch",
    "dev": "webpack-dev-server --devtool eval --progress --colors --content-base app/ --history-api-fallback --open chrome"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/webdemos/cucumber-demo.git"
  },
